{
    "name": "met-museum-tour",
    "repository": {
        "type": "git",
        "url": "https://github.com/juliamcneill/met-museum-tour.git"
    },
    "description": "Application that generates a customized tour of the MET Museum of Art based on a user's interests.",
    "author": "Julia McNeill",
    "license": "UNLICENSED",
    "engines": {
        "node": "18"
    },
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.13",
        "@mui/material": "^5.14.4",
        "axios": "^1.4.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.1.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/lodash": "^4.14.198",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "commitlint-config-jira": "^1.6.4",
        "commitlint-plugin-jira-rules": "^1.6.4",
        "eslint": "^8.45.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "postcss": "^8.4.31",
        "prettier": "^3.0.1",
        "prettier-plugin-tailwindcss": "^0.5.2",
        "stylelint": "^15.10.2",
        "stylelint-config-standard": "^34.0.0",
        "tailwindcss": "^3.3.3",
        "vite": "^4.4.5"
    },
    "lint-staged": {
        "*": [
            "npx prettier --write"
        ],
        "*.{ts,tsx}": [
            "echo",
            "npx eslint --cache"
        ],
        "*.{css,scss}": [
            "echo",
            "npx stylelint"
        ]
    },
    "commitlint": {
        "plugins": [
            "commitlint-plugin-jira-rules"
        ],
        "extends": [
            "jira"
        ]
    }
}
